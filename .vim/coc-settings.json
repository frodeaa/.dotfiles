{
  "prettier.tabWidth": 4,
  "prettier.trailingComma": "all",
  "prettier.tslintIntegration": true,
  "python.jediEnabled": false,
  "go.commandPath": "~/go/bin/gopls",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
